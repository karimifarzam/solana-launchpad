{
  "name": "solana-launchpad",
  "version": "0.1.0",
  "description": "Solana token launchpad with bonding curves and Meteora integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:program": "anchor build",
    "test": "anchor test --skip-local-validator",
    "test:unit": "jest",
