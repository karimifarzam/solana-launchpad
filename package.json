    "deploy:testnet": "anchor deploy --provider.cluster testnet",
    "generate-types": "anchor idl parse --file target/idl/launchpad.json --out app/src/idl",
    "dev": "next dev",
    "build:app": "next build",
    "start": "next start",
    "lint": "next lint"
  },
